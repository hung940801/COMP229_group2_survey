<% if (typeof displayName == "string" && displayName) { %>
    <a href="/surveys/add" class="btn btn-primary" role="button"><i class="fas fa-plus-circle"></i>&nbsp;Add a Survey</a>
<% } %>
<br>
<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th class="text-content col-2">Name</th>
            <th class="text-content col-4">Description</th>
            <% if (typeof displayName == "string" && displayName) { %>
                <th class="text-content col-1">Edit</th>
                <th class="text-content col-1">Delete</th>
            <% } %>
        </tr>
    </thead>
    <tbody>
        <%for(let count = 0; count < surveyList.length; count++) {%>
            <tr>
                <td class="text-center col-2"><%= surveyList[count].name %></td>
                <td class="text-center col-4"><%= surveyList[count].description %></td>
                <% if (typeof displayName == "string" && displayName) { %>
                    <td class="text-center col-1"><a href="/surveys/edit/<%= surveyList[count].id %>" class="btn btn-primary"><i class="fas fa-pencil-alt"></i>&nbsp;Edit</a></td>
                    <td class="text-center col-1"><a href="/surveys/delete/<%= surveyList[count].id %>" class="btn btn-danger"><i class="fas fa-trash-alt"></i>&nbsp;Delete</a></td>
                <% } %>
            </tr>
        <%}%>
    </tbody>
</table>
